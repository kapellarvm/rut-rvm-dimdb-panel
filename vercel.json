{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "regions": ["fra1"],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://rut.krvmservice.com/$1",
      "permanent": true
    }
  ]
}
